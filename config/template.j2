/**
 * @file
 * Defines default strategy parameter values for the given timeframe.
 */

// Defines indicator's parameter values for the given pair symbol and timeframe.
struct Indi_DPO_Params_{{ Timeframe }} : DPOIndiParams {
  Indi_DPO_Params_{{ Timeframe }}() : DPOIndiParams(indi_dpo_defaults, PERIOD_{{ Timeframe }}) {
    shift = {{ DPO_Indi_DPO_Shift | default(0) }};
  }
} indi_dpo_{{ Timeframe | lower }};

// Defines strategy's parameter values for the given pair symbol and timeframe.
struct Stg_DPO_Params_{{ Timeframe }} : StgParams {
  // Struct constructor.
  Stg_DPO_Params_{{ Timeframe }}() : StgParams(stg_dpo_defaults) {
    lot_size = {{ DPO_LotSize | default(0) }};
    signal_open_method = {{ DPO_SignalOpenMethod | default(0) }};
    signal_open_filter_method = {{ DPO_SignalOpenFilterMethod | default(1) }};
    signal_open_level = (float){{ DPO_SignalOpenLevel | default(0) }};
    signal_open_boost = {{ DPO_SignalOpenBoostMethod | default(0) }};
    signal_close_method = {{ DPO_SignalCloseMethod | default(0) }};
    signal_close_level = (float){{ DPO_SignalCloseLevel | default(0) }};
    price_stop_method = {{ DPO_PriceStopMethod | default(0) }};
    price_stop_level = (float){{ DPO_SignalCloseLevel | default(1) }};
    tick_filter_method = {{ DPO_TickFilterMethod  | default(1) }};
    max_spread = {{ DPO_MaxSpread | default(0) }};
  }
} stg_dpo_{{ Timeframe | lower }};
